{
  "name": "datalink",
  "version": "1.0.1",
  "description": "Datalink - A library that makes database access easy and fun",
  "main": "./dist/index.js",
  "files": [
    "dist/database.js",
    "dist/engine",
    "dist/filter.js",
    "dist/index.js",
    "dist/misc.js",
    "dist/model.js",
    "dist/flush.js",
    "dist/tree.js",
    "bin/datalink.js",
    "src",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "author": "Weidong Fang",
  "license": "MIT",
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "http://github.com/fangwd/datalink.git"
  },
  "devDependencies": {
    "@types/node": "^9.4.7",
    "@types/jest": "^22.2.0",
    "@types/mysql": "^2.15.4",
    "jest": "^22.4.1",
    "ts-jest": "^22.4.1",
    "typescript": "*",
    "mysql": "^2.15.0"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/test/.+\\.test.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "verbose": true
  }
}
